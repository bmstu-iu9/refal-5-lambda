* TREE

$ENTRY Go {
  /* empty */
    = <F 1 /* empty */> : A 1
    = <F 2 2> : B 2
    = <F 3 1 2 3 4> : C 3
    = <F 4 'abc'> : 'abc'
    = /* empty */;
}

*$SPEC F;

F {
  s.Mark e.X
    = e.X
    : {
        /* empty */ = A s.Mark;
        s.Mark = B s.Mark;
        e._ s.Mark e._ = C s.Mark;
        e._ = e.X;
      }
}
